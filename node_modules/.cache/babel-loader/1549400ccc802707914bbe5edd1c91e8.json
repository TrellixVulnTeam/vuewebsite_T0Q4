,S;;;;OADH,CAiBI,E,CAVK,IAAA,CAAA,U,kBAJT,YAAA,CAKE,OALF,EAAA,WAAA,C,qCAcI,EARyD,IAAA,CAAA,U,kBAA7D,YAAA,CAOS,QAPT,EAAA,WAAA,EAOS,CANP,WAMO,E,WAAA,CAPT,C,qCAQI,CAlBN,CApCmE,CAAnE;AAAA,K;;;;GADF,E;;AAAA,I,iCAAA,CAyDI,CA3DN,CA4DI,CAhEN,CAiEI,CAlEN,C","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"card\">\n      <div class=\"card-header\">\n        <h6>Add Product</h6>\n      </div>\n      <div class=\"card-body\">\n        <!-- <form v-on:submit.prevent=\"onSaveProduct\" :validation-schema=\"schema\"> -->\n        <Form @submit=\"onSubmit\" :validation-schema=\"schema\">\n          <!-- <Form @submit=\"onSaveProduct\" :validation-schema=\"schema\"> -->\n          <div class=\"form-group row\">\n            <div class=\"col-6\">\n              <label>Product Name:</label>\n              <Field\n                id=\"title\"\n                name=\"title\"\n                type=\"text\"\n                class=\"form-control\"\n                v-model=\"product.title\"\n              />\n              <ErrorMessage name=\"title\" class=\"text-danger\" />\n            </div>\n            <div class=\"col-6\">\n              <label>Product Price:</label>\n              <Field\n                name=\"price\"\n                type=\"number\"\n                class=\"form-control\"\n                v-model=\"product.price\"\n              />\n              <ErrorMessage name=\"price\" class=\"text-danger\" />\n            </div>\n          </div>\n          <div class=\"form-group row\">\n            <div class=\"col-12\">\n              <label>Product Details:</label>\n              <Field\n                name=\"description\"\n                as=\"textarea\"\n                class=\"form-control\"\n                v-model=\"product.description\"\n               />\n              <ErrorMessage name=\"description\" class=\"text-danger\" />\n            </div>\n          </div>\n          <div class=\"form-group\">\n            <router-link to=\"/products\" class=\"btn btn-secondary mr-2\"\n              >Cancel</router-link\n            >\n            <input\n              type=\"submit\"\n              class=\"btn btn-primary\"\n              value=\"Add Product\"\n              v-if=\"!isCreating\"\n            />\n            <button class=\"btn btn-primary\" type=\"button\" disabled v-if=\"isCreating\">\n              <span\n                class=\"spinner-border spinner-border-sm\"\n                role=\"status\"\n                aria-hidden=\"true\"\n              ></span>\n              Saving...\n            </button>\n          </div>\n        </Form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\nimport { Field, Form, ErrorMessage } from \"vee-validate\";\nimport * as yup from \"yup\";\n\nexport default {\n  data() {\n    return {\n      product: {}\n    };\n  },\n\n  components: {\n    Field,\n    Form,\n    ErrorMessage,\n  },\n\n  computed: { ...mapGetters([\"isCreating\", \"createdData\"]) },\n\n  methods: {\n    ...mapActions([\"storeProduct\"]),\n    onSubmit() {\n      const { title, price, description } = this.product;\n      this.storeProduct({\n        title: title,\n        price: price,\n        image: null,\n        description: description,\n        user_id: 1,\n      });\n    }\n  },\n\n  watch: {\n    createdData: function () {\n      if (this.createdData !== null && !this.isCreating) {\n        this.$swal.fire({\n          text: \"Success, Product has been added.\",\n          icon: \"success\",\n          position: \"top-end\",\n          timer: 1000,\n        });\n\n        this.$router.push({ name: \"Products\" });\n      }\n    },\n  },\n\n  setup() {\n    // Define a validation schema\n    const schema = yup.object({\n      title: yup.string().required().min(5),\n      price: yup.string().required(),\n      description: yup.string().required().min(5),\n    });\n\n    return {\n      schema\n    };\n  },\n};\n</script>\n"],"sourceRoot":""}]}