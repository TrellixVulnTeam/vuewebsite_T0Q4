  if (result[\"isConfirmed\"]) {\n            // Put delete logic\n            this.deleteProduct(id);\n            this.fetchAllProducts({\n              page: 1,\n              search: ''\n            });\n            this.$swal.fire({\n              text: \"Success, Product has been deleted.\",\n              icon: \"success\",\n              position: 'top-end',\n              timer: 1000,\n            });\n          }\n        });\n    },\n  },\n};\n</script>\n"]}]}